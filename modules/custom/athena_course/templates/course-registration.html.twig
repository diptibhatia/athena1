<div class="container-fluid p-0 about-us-wrapper">

        <div class="container bg-white login-wrapper">
            <div class="row">                
                <div class="col-md-6 col-sm-12">
                    <h1>Register <span>Now</span></h1>
                    <h3>Start Learning in 60 Seconds - No need for any payment details!</h3>
                    <h4>If you are a  Existing user please <span>Sign In</span></h4>
                    <div class="login-details">
                        <div class="login-item names">
                            <div class="name-item">
                                <p>First Name</p>
                                <input type="text" placeholder="Enter First Name">
                            </div>
                            <div class="name-item">
                                <p>Last Name</p>
                                <input type="text" placeholder="Enter Last Name">
                            </div>
                        </div>
                        <div class="login-item">
                            <p>Email</p>
                            <input type="email" placeholder="Enter Email Address">
                        </div>
                        <div class="login-item">
                            <p>Mobile Number</p>
                            <input type="number" placeholder="Enter Mobile number">
                        </div>
                        <div class="login-item">
                            <p>Choose your Course</p>
                            <select name="course" id="course">
                                {% for nodedata in nodes %}
                                 <option value="{{nodedata.id}}" default="">{{nodedata.label}}</option>
                                    {% endfor %}
                            </select>
                        </div>
                        <div class="login-item">
                            <p>Highest Qualification</p>
                            <select name="course" id="course">
                                <option value="course 1" default="">course 1</option>
                                <option value="course 2">course 2</option>
                                <option value="coourse 3">course 3</option>
                            </select>
                        </div>
                        <div class="login-item names">
                            <div class="name-item">
                                <p>Year of Experience</p>
                                <input type="number" placeholder="YY">
                            </div>
                            <div class="name-item">
                                <p>&nbsp;</p>
                                <input type="number" placeholder="MM">
                            </div>
                        </div>
                        <div class="login-item">
                            <p>Level of Employment</p>
                            <input type="text" placeholder="Employment Level">
                        </div>
                        <div class="login-item">
                            <p>Password</p>
                            <input type="password" placeholder="Password">
                        </div>
                        <div class="login-item">
                            <p>Confirm Password</p>
                            <input type="password" placeholder="Confirm Password">
                        </div>
                        <!-- <div class="forgot-password">
                            Forgot Password?
                        </div> -->
                        <div class="button">
                            <input type="checkbox" id="vehicle1" name="vehicle1">
                            <label for="vehicle1"> I agree with Terms &amp; Conditions</label>
                            <button>Register</button>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-12 register-images">
                    <img src="{{base_path}}/images/Register-athena-img.png" alt="login image">
                </div>
            </div>
        </div>
    </div>