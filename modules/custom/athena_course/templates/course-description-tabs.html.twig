<div class="partners-tabs">
        <div class="wrapper">
            <h1>Partners of this Programme</h1>
            <div class="classic-tabs"> <!-- Bootstrap Tabs -->

                {% set nummact = 1 %}
 {% set activeclass = '' %}
                <ul class="nav tabs-cyan" id="myClassicTab" role="tablist">
                 {% for universities in univ_data	 %}
                 
                  {% if nummact == 1 %} 
                  {% set activeclass = 'active show' %}
                  {% else %}
                        {% set activeclass = '' %}
                    {% endif %}
                    <li class="nav-item">
                        <a class="nav-link  waves-light {{activeclass}}" id="guglielmo-tab-classic" data-toggle="tab" href="#guglielmo{{nummact}}" role="tab" aria-controls="guglielmo-classic" aria-selected="true">
                        {{ universities.university.label }}</a>
                    </li>
                     {% set nummact = nummact+1 %}
                 {% endfor %}       
                   
                </ul>
                
                
                
                <div class="tab-content rounded-bottom" id="myClassicTabContent">
                  {% set numm = 1 %}
                  {% for universities in univ_data	 %}
                  {% if numm == 1 %} 
                  {% set activeclasstab = 'active show' %}
                  {% else %}
                        {% set activeclasstab = '' %}
                    {% endif %}
                
                    <div class="tab-pane fade {{activeclasstab}}" id="guglielmo{{numm}}" role="tabpanel" aria-labelledby="guglielmo-tab-classic">
                        <div class="partners-tabs__mba">
                            <div class="partners-tabs__mba-text">
                                <div class="content">
                                    <h2>{{course_title}}</h2>
                                    <p>{{universities.university.field_description.value|raw}} </p>
                                    {% if universities.univ_logo is empty %}
                                    <img src="{{base_path}}/images/coursepage/gmu-logo.svg" alt="uni-logo">
                                    {% else %}
                                    <img src="{{universities.univ_logo}}" alt="uni-logo">
                                        {% endif %}
                                </div>
                                <div class="certificate">
                                    <div id="carouselExampleIndicators{{numm}}" class="carousel slide" data-ride="carousel" data-interval="false">
                                        {% set carouselnum = 1 %}
                                        <div class="carousel-inner">
                                           {% for certificate in universities.certificates %}
                                         {% if carouselnum == 1 %} 
                  {% set carouselnumclass = 'active' %}
                  {% else %}
                        {% set carouselnumclass = '' %}
                    {% endif %}
                    
                    
                                        
                                            <div class="carousel-item {{carouselnumclass}} ">
                                                <!-- Button to trigger Modal -->
                                                <button type="button" class="cover" data-toggle="modal" data-target="#certificate{{numm}}{{carouselnum}}">
                                                    <img class="d-block w-100" src="{{ certificate}}" alt="First slide">
                                                </button>
                                                <!-- Modal -->
                                                <div class="modal fade" id="certificate{{numm}}{{carouselnum}}" tabindex="-1" aria-labelledby="certificate1ModalLabel" aria-hidden="true">
                                                    <div class="modal-dialog">
                                                        <div class="modal-content">
                                                            <div class="modal-header">
                                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                <span aria-hidden="true">×</span>
                                                            </button>
                                                            </div>
                                                            <div class="modal-body">
                                                                <img src="{{ certificate}}" alt="First slide">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                    
                                            
{% set carouselnum =  carouselnum+1 %}                    
                                        
                                        
                                        {% endfor %}
                                        </div>
                                        
                                        
                                        
                                        <a class="carousel-control-prev" href="#carouselExampleIndicators{{numm}}" role="button" data-slide="prev">
                                            <img src="{{base_path}}/images/coursepage/prev.svg" alt="previous">
                                            <!-- <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                            <span class="sr-only">Previous</span> -->
                                        </a>
                                        <a class="carousel-control-next" href="#carouselExampleIndicators{{numm}}" role="button" data-slide="next">
                                            <img src="{{base_path}}/images/coursepage/next.svg" alt="next">
                                            <!-- <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                            <span class="sr-only">Next</span> -->
                                        </a>
                                    </div>
                                    <p>Click to Enlarge</p>
                                </div>
                                <p class="note">*All certificate and transcript images are for illustrative purposes only and may be 
                                    subject to change at the discretion of respective Universities or Awarding Bodies.</p>
                            </div>
                            <div class="partners-tabs__mba-img"></div>
                        </div>
                        
                        {% if universities.message|trim is not empty %}
                        <div class="partners-tabs__msg">
                            <div class="partners-tabs__msg-person">
                                <img src="{{base_path}}/images/coursepage/rector.svg" alt="msg author">
                                <h3>{{universities.msg_title}}</h3>
                                <p>{{universities.prof_name}}</p>
                                <p>{{prof_univ}}</p>
                            </div>
                            <div class="partners-tabs__msg-text">
                               {{universities.message|raw}}
                            </div>
                        </div>
                        <div class="partners-tabs__msg partners-tabs__msgvideo">
                            <div class="partners-tabs__msg-video partners-tabs__msgvideo-video">
                                <div class="video">
                                    <!-- change this test video with real video -->
                                    <iframe src=" {{universities.dean_video}}" width="640" height="480" frameborder="0" allow="autoplay; fullscreen" allowfullscreen=""></iframe>
                                </div>
                            </div>
                            <div class="partners-tabs__msg-person partners-tabs__msgvideo-person">
                                <img src="{{base_path}}/images/coursepage/dean.svg" alt="Dean">
                                <h3>Message from the Dean</h3>
                                <p>{{dean_univ}}</p>
                            </div>
                        </div>
                        
                       {% endif %} 
                    </div>
                    
                   
                   
                    {% set numm = numm+1 %}
                    {% endfor %} 
                   
                   
                   
                </div>

            </div>
        </div>
    </div>
    
    
