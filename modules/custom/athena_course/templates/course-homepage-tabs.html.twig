<div class="container-fluid my-container">
             <div class="explore-courses">
                <div class="wrapper">
                   <h2>Explore our Courses</h2>
                   <div class="course-text-info">
                      <p>
                         We have a multitude of courses tailored to your career goals and busy schedule. These courses have been developed to enhance your knowledge and critical thinking abilities and make you an expert in your domain.
                      </p>
                   </div>
                </div>
                <div class="tabs">
                   <ul class="nav tabs-cyan tab-list" id="myClassicTab" role="tablist">
                      <li class="nav-item tab-item">
                         <a class="nav-link waves-light show active" id="overview-tab-classic" data-toggle="tab" href="#acadamic" role="tab" aria-controls="overview-classic" aria-selected="true">Academic</a>
                      </li>
                      {% if micro is not empty %}
                      <li class="nav-item tab-item">
                         <a class="nav-link waves-light" id="course-module-tab-classic" data-toggle="tab" href="#micro-credits" role="tab" aria-controls="course-module-classic" aria-selected="false">Micro Credits</a>
                      </li>
                      {% endif %}
                      {% if certifications is not empty %}
                      <li class="nav-item tab-item">
                         <a class="nav-link waves-light" id="eligibility-tab-classic" data-toggle="tab" href="#certifications" role="tab" aria-controls="eligibility-classic" aria-selected="false">Certifications</a>
                      </li>
                      {% endif %}
                   </ul>
                </div>
                <div class="tab-content rounded-bottom" id="myClassicTabContent">
                   <div class="tab-pane fade active show" id="acadamic" role="tabpanel" aria-labelledby="acadamic-tab-classic">
                      <div class="fyi-tabs__overview">
                         <div class="courses-offered">
                            <div class="info">
                               <h2>Our Academic Courses</h2>
                               <p>{{acaddesc | raw}}
                               </p>
                               <form action="{{ url('<front>') }}academic-program" method="POST">
                                <input type="hidden" name="course_category" value="Academic">
                                <input type="hidden" name="submit_form" value="submit_form">
                               <div class="hideinMobileView"><button>View All Courses</button></div>
                               </form>
                            </div>
                            <div class="courses">
                               <div class="course-list">
                                 <div class="owl-carousel owl-theme partner-logos">
                                   {% for academicdata in academic %}
                                    <div class="item">
                                      <div class="course-item">
                                         <div class="image">
                                          {% if academicdata.field_search_list_image.entity.uri.value is not empty %}


        <a href="{{ url('<front>') }}course/{{academicdata.id}}"> <img class="course-img" src=" {{ file_url(academicdata.field_search_list_image.entity.uri.value)}}" alt="Course"></a>
         {% else %}
          <img class="course-img" src="/themes/custom/athena/images/course-image2.png" alt="Course">
           {% endif %}
                                         </div>
                                         <div class="price-tag">
                                            <p>${{academicdata.field_course_total_fee.value}}</p>
                                         </div>
                                         <div class="course-details">
                                            <h1 class="headerWrap">{{academicdata.label}}</h1>
                                            <h2>{{academicdata.field_course_awarding_body.value|raw }}</h2>

                                            {% if ( academicdata.field_course_level.value is empty ) and ( academicdata.field_course_ects_credit.value is empty ) and ( academicdata.field_courses_credit_type.value is empty ) %}

                                            <div class="course-tags-empty"></div>

                                            {% else %}

                                            <div class="course-tags">

                                               <p>

                                                {% if academicdata.field_course_level.value is not empty %}<span class="circle"></span>
                                                  {{academicdata.field_course_level.value}}
                                                {% endif %}
                                                

                                                {% if academicdata.field_course_ects_credit.value is not empty %}
                                                  <span>|</span>
                                                  {{academicdata.field_course_ects_credit.value}}
                                                  {% if academicdata.field_courses_credit_type.value is not empty %}
                                                  {{academicdata.field_courses_credit_type.value}}
                                                {% endif %}

                                                {% endif %}                                           

                                               </p>
                                            </div>
                                            {% endif %}

                                            <h3><b>Duration : </b> {{academicdata.field_course_duration.value}}</h3>
                                         </div>
                                         <div class="course-info">
                                           {{academicdata.field_course_banner_description.value | truncate(60, true)}} {{'...'}} <span><a style="color: inherit;" href="{{ url('<front>') }}course/{{academicdata.id}}" >Read More</a></span>
                                         </div>
                                         <div class="know-more">
                                            <p><a style="color:inherit;" href="{{ url('<front>') }}course/{{academicdata.id}}" >Know More</a></p>
                                         </div>
                                      </div>
                                    </div>
                                  {% endfor %}
                                  </div>
                               </div>
                            </div>
                         </div>
                      </div>

                      <!--Button will be vissible on mobile onle : START-->
                        <div class="container">
                           <div class="col-12">
                              <form action="{{ url('<front>') }}academic-program" method="POST">
                                 <input type="hidden" name="course_category" value="Academic">
                                 <input type="hidden" name="submit_form" value="submit_form">
                                 <button class="explore d-inline d-md-none d-lg-none">View All Courses</button>
                              </form>
                           </div>
                        </div>
                      <!--Button will be vissible on mobile onle : END-->

                   </div>

                   {% if micro is not empty %}
                   <div class="tab-pane fade" id="micro-credits" role="tabpanel" aria-labelledby="micro-credits-tab-classic">
                      <div class="fyi-tabs__micro-credits">
                         <div class="courses-offered">
                            <div class="info">
                               <h2>Our Micro Credits Courses</h2>
                               <p>{{microdesc | raw}}
                               </p>
                               <form class="top-search-section__search" action="{{ url('<front>') }}micro-credits" method="POST">
                                <input type="hidden" name="course_category" value="Micro Credits">
                                <input type="hidden" name="submit_form" value="submit_form">
                               <div class="hideinMobileView"><button>View All Courses</button></div>
                               </form>
                            </div>
                            <div class="courses">
                               <div class="course-list">
                                  <div class="course-list">
                                    <div class="owl-carousel owl-theme partner-logos">
                                       {% for microdata in micro %}
                                        <div class="item">
                                          <div class="course-item">
                                             <div class="image">
                                               {% if microdata.field_search_list_image.entity.uri.value is not empty %}


        <a href="{{ url('<front>') }}course/{{microdata.id}}"> <img class="course-img" src=" {{ file_url(microdata.field_search_list_image.entity.uri.value)}}" alt="Course"></a>
         {% else %}
          <img class="course-img" src="/themes/custom/athena/images/course-image2.png" alt="Course">
           {% endif %}
                                             </div>
                                             <div class="price-tag">
                                                <p>${{microdata.field_course_total_fee.value}}</p>
                                             </div>
                                             <div class="course-details">
                                                <h1>{{microdata.label}}</h1>
                                                <h2>{{microdata.field_course_awarding_body.value|raw }}</h2>
                                                <div class="course-tags">

                                                   <p><span class="circle"></span>Masters ( EQF Level 7) <span>|</span>{{microdata.field_course_ects_credit.value}} Credits (ECTS)</p>
                                                </div>
                                                <h3><b>Duration : </b> {{microdata.field_course_duration.value}}</h3>
                                             </div>
                                             <div class="course-info">
                                               {{microdata.field_course_banner_description.value | truncate(60, true)}} {{'...'}} <span><a style="color: inherit;" href="{{ url('<front>') }}course/{{academicdata.id}}" >Read More</a></span>
                                             </div>
                                             <div class="know-more">
                                                <p><a style="color:inherit" href="{{ url('<front>') }}course/{{microdata.id}}" >Know More</a></p>
                                             </div>
                                          </div>
                                        </div>
                                      {% endfor %}
                                    </div>
                               </div>

                               </div>
                            </div>
                         </div>
                      </div>


                        <!--Button will be vissible on mobile onle : START-->
                        <div class="container">
                           <div class="col-12">
                                 <form class="top-search-section__search" action="{{ url('<front>') }}micro-credits" method="POST">
                                       <input type="hidden" name="course_category" value="Micro Credits">
                                       <input type="hidden" name="submit_form" value="submit_form">
                                 <button class="explore d-inline d-md-none d-lg-none" >View All Courses</button>
                              </form>
                           </div>
                        </div>
                        <!--Button will be vissible on mobile onle : END-->


                   </div>
                    {% endif %}
                   {% if certifications is not empty %}
                   <div class="tab-pane fade" id="certifications" role="tabpanel" aria-labelledby="certifications-tab-classic">
                      <div class="fyi-certifications">
                         <div class="courses-offered">
                            <div class="info">
                               <h2>Our Certification Courses</h2>
                               <p>{{cerdesc | raw}}
                               </p>
                                <form class="top-search-section__search" action="{{ url('<front>') }}certifications" method="POST">
                                <input type="hidden" name="course_category" value="Certifications">
                                <input type="hidden" name="submit_form" value="submit_form">
                               <div class="hideinMobileView"><button>View All Courses</button></div>
                               </form>
                            </div>
                            <div class="courses">
                               <div class="course-list">
                                  <div class="owl-carousel owl-theme partner-logos">
                                    {% for certificationsdata in certifications %}
                                      <div class="item">
                                        <div class="course-item">
                                           <div class="image">
                                               {% if certificationsdata.field_search_list_image.entity.uri.value is not empty %}


        <a href="{{ url('<front>') }}course/{{certificationsdata.id}}"> <img class="course-img" src=" {{ file_url(certificationsdata.field_search_list_image.entity.uri.value)}}" alt="Course"></a>
         {% else %}
          <img class="course-img" src="/themes/custom/athena/images/course-image2.png" alt="Course">
           {% endif %}
                                           </div>
                                           <div class="price-tag">
                                              <p>${{certificationsdata.field_course_total_fee.value}}</p>
                                           </div>
                                           <div class="course-details">
                                              <h1>{{certificationsdata.label}}</h1>
                                              <h2>{{certificationsdata.field_course_awarding_body.value|raw }}</h2>

                                              {% if ( certificationsdata.field_course_level.value is empty ) and ( certificationsdata.field_course_ects_credit.value is empty ) and ( certificationsdata.field_courses_credit_type.value is empty ) %}

                                            <div class="course-tags-empty"></div>

                                            {% else %}

                                              <div class="course-tags">

                                                 <p>

                                                  {% if certificationsdata.field_course_level.value is not empty %}<span class="circle"></span>
                                                    {{certificationsdata.field_course_level.value}}
                                                  {% endif %}

                                                  

                                                  {% if certificationsdata.field_course_ects_credit.value is not empty %}<span>|</span>
                                                    {{certificationsdata.field_course_ects_credit.value}}
                                                    {% if certificationsdata.field_courses_credit_type.value is not empty %}
                                                    {{certificationsdata.field_courses_credit_type.value}}
                                                  {% endif %}

                                                  {% endif %}

                                                  
                                                 </p>
                                              </div>
                                              {% endif %}

                                              <h3><b>Duration : </b> {{certificationsdata.field_course_duration.value}}</h3>
                                           </div>
                                           <div class="course-info">
                                             {{certificationsdata.field_course_banner_description.value | truncate(60, true)}} {{'...'}} <span><a style="color: inherit;" href="{{ url('<front>') }}course/{{certificationsdata.id}}" >Read More</a></span>
                                           </div>
                                           <div class="know-more">
                                              <p><a style="color:inherit" href="{{ url('<front>') }}course/{{certificationsdata.id}}" >Know More</a></p>
                                           </div>
                                        </div>
                                      </div>
                                    {% endfor %}
                                  </div>
                               </div>
                            </div>
                         </div>
                      </div>


                        <!--Button will be vissible on mobile onle : START-->
                        <div class="container">
                           <div class="col-12">
                              <form class="top-search-section__search" action="{{ url('<front>') }}certifications" method="POST">
                                 <input type="hidden" name="course_category" value="Certifications">
                                 <input type="hidden" name="submit_form" value="submit_form">
                                 <button class="explore d-inline d-md-none d-lg-none">View All Courses</button>
                              </form>
                           </div>
                        </div>
                      <!--Button will be vissible on mobile onle : END-->


                   </div>

                   {% endif %}
                </div>
             </div>
        </div>

