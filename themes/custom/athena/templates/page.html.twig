<header class="site-header">
    <div class="wrapper">
    {{ page.header }}
</header>
<div class="top-gap"></div>
{% if page.highlighted %}
    <div class="highlighted">
    <aside class="layout-container section clearfix" role="complementary">
        {{ page.highlighted }}
    </aside>
    </div>
{% endif %}

{% if athena_exclude or error_page == node.id or node.id == 363 %}
  {{ page.content }}
{% else %}
    <div class="main-wrapper container-fluid p-0 about-us-wrapper">
    {% if page.featured_top %}
            {{ page.featured_top }}
    {% endif %}
      <div class="pt-4 container bg-white pl-0 pr-0">
        <div class="row m-0">
          {% if page.sidebar_first %}
              <div class="col-md-4 col-sm-12 pb-5">
                  {{ page.sidebar_first }}
              </div>
          {% endif %}
          <div class="col-lg-8">
              {{ page.content }}
          </div>
          {% if page.sidebar_second %}
              <div class="col-md-4 col-sm-12">
                  {{ page.sidebar_second }}
              </div>
          {% endif %}
        </div>
      </div>
    </div>
{% endif %}
{% if page.featured_bottom %}
    <div class="featured-bottom">
    <aside class="layout-container clearfix" role="complementary">
        {{ page.featured_bottom }}
    </aside>
    </div>
{% endif %}

{% if exclude_footer == false %}
{{footercoursemenu|raw}}
{{ get_static_data('footer_links', TRUE)|raw }}
{% endif %}

