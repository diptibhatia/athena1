<style>
  .insightsgif > img{ width:100% }
</style>

{%
  set title_classes = [
    label_display == 'visually_hidden' ? 'visually-hidden',
  ]
%}

{% if label_hidden %}
  {% if multiple %}
    <div class="insightsgif">
      {% for item in items %}
        {{ item.content }}
      {% endfor %}
  {% else %}
    {% for item in items %}
      <div class="insightsgif">{{ item.content }}
    {% endfor %}
  {% endif %}
{% else %}
    {% if multiple %}
    {% endif %}
    {% for item in items %}
      <div class="insightsgif">{{ item.content }}
    {% endfor %}
    {% if multiple %}
    {% endif %}
{% endif %}
