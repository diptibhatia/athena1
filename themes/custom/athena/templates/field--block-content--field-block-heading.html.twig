{%
  set title_classes = [
    label_display == 'visually_hidden' ? 'visually-hidden',
  ]
%}

{% if label_hidden %}
  {% if multiple %}
      {% for item in items %}
        {{ item.content }}</h1>
      {% endfor %}
  {% else %}
    {% for item in items %}
        {{ item.content }}</h1>
    {% endfor %}
  {% endif %}
{% else %}
    {% if multiple %}
    {% endif %}
    {% for item in items %}
      {{ item.content }}</h1>
    {% endfor %}
    {% if multiple %}
    {% endif %}
{% endif %}
