{% if label_hidden %}
  {% if multiple %}
    <div{{ attributes }} class="col-md-12">
      {% for item in items %}
        <div{{ item.attributes }}>{{ item.content }}
      {% endfor %}
    </div>
  {% else %}
    {% for item in items %}
      <div{{ attributes }} class="col-md-12">{{ item.content }}
    {% endfor %}
  {% endif %}
{% else %}
  <div{{ attributes }} class="col-md-12">
    <div{{ title_attributes.addClass(title_classes) }} class="col-md-12">{{ label }}
    {% if multiple %}
      <div>
    {% endif %}
    {% for item in items %}
      <div{{ item.attributes }} class="col-md-12">{{ item.content }}
    {% endfor %}
    {% if multiple %}
      </div>
    {% endif %}
  </div>
{% endif %}
